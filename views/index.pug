							.image-block
								img.img-fluid(src='img/pablo-delivery.png' alt='Awesome Image')

	section.space-sm
		.container
			.row
				.col-12.alert.alert-warning
					| Below is content generated from the Space Manager database link.
			.row
				.col-12.alert
					each _site,index in sites
						div(id=index class="sites") #{_site.site}
							
			.row
				.col-12.alert.alert-warning
					| End dynamic content.

	section.space-sm
		.container
			.row
				div#map(style="width: 100%; height: 580px")

	section.space-md
